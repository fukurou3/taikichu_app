{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Flutter: Run Web",
      "type": "shell",
      "command": "flutter",
      "args": ["run", "-d", "chrome"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    }
  ]
}